        <header id="gtco-header" class="gtco-cover gtco-cover-sm" role="banner" style="background-image: url(https://st3.depositphotos.com/1027198/13417/i/1600/depositphotos_134178444-stock-photo-health-food-or-junk-food.jpg)" data-stellar-background-ratio="0.5">
            <div class="overlay"></div>
            <div class="gtco-container">
                <div class="row">
                    <div class="col-md-12 col-md-offset-0 text-center">

                        <div class="row row-mt-15em">
                            <div class="col-md-12 mt-text animate-box" data-animate-effect="fadeInUp">
                                
                                <h1 class="cursive-font">Cal your calorie!</h1>
                            </div>

                        </div>


                    </div>
                </div>
            </div>
        </header>


        <div class="gtco-section">
            <div class="gtco-container">
                <div class="row">
                    <div class="col-md-8 col-md-offset-2 text-center gtco-heading">
                        <h2 class="cursive-font primary-color">Today's menu</h2>
                        <p>Dignissimos asperiores vitae velit veniam totam fuga molestias accusamus alias autem provident. Odit ab aliquam dolor eius.</p>
                    </div>
                </div>
                
                <div class="row">
                    @{
                        foreach (dynamic sp in ViewBag.myRecipes)
                        {
                            <div class="col-lg-4 col-md-4 col-sm-6">
                                
                                    <figure>
                                        <div class="overlay"><i class="ti-plus"></i></div>
                                        <img src="@sp.ImageUrl" alt="Image" class="img-responsive">
                                    </figure>
                                    <div class="fh5co-text">
                                        <h2>@sp.title</h2>
                                        <p>Far far away, behind the word mountains, far from the countries Vokalia..</p>
                                        <p><span class="price cursive-font">@sp.Calorie KCal</span></p>
                                    </div>
                               
                            </div>
                        }
                    }
                   
                    
                    
                </div>
            </div>
        </div>
        <div class="gtco-cover gtco-cover-sm" style="background-image: url(images/img_bg_1.jpg)" data-stellar-background-ratio="0.5">
            <div class="overlay"></div>
            <div class="gtco-container text-center">
                <div class="display-t">
                    <div class="display-tc">
                        <div id="chart">
                            <script src="~/js/GetNutrition.js"></script>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="gtco-counter" class="gtco-section">
            <div class="gtco-container">
                <div class="row">
                    <div class="col-md-8 col-md-offset-2 text-center gtco-heading animate-box">
                        <h2 class="cursive-font primary-color"> To burn off the KCal</h2>
                        <p>Recommended amount of exercise for today!</p>
                    </div>
                </div>
                <div class="row">
                    @{
                                int a = -ViewBag.target;
                                if (ViewBag.target > 0)
                                {
                            <div class="col-md-3 col-sm-6 animate-box" data-animate-effect="fadeInUp">
                                <div class="feature-center">
                                    <span class="counter js-counter" data-from="0" data-to="@ViewBag.exercise[0]" data-speed="3000" data-refresh-interval="50">1</span>
                                    <span class="counter-label">Walking/mile</span>
                                </div>
                            </div>
                            <div class="col-md-3 col-sm-6 animate-box" data-animate-effect="fadeInUp">
                                <div class="feature-center">
                                    <span class="counter js-counter" data-from="0" data-to="@ViewBag.exercise[1]" data-speed="3000" data-refresh-interval="50">1</span>
                                    <span class="counter-label">Running/mile</span>
                                </div>
                            </div>
                            <div class="col-md-3 col-sm-6 animate-box" data-animate-effect="fadeInUp">
                                <div class="feature-center">
                                    <span class="counter js-counter" data-from="0" data-to="@ViewBag.exercise[2]" data-speed="3000" data-refresh-interval="50">1</span>
                                    <span class="counter-label">Bicycling/mile</span>
                                </div>
                            </div>
                           <div class="col-md-3 col-sm-6 animate-box" data-animate-effect="fadeInUp">
                                <div class="feature-center">
                                    <span class="counter js-counter" data-from="0" data-to="@ViewBag.target" data-speed="2000" data-refresh-interval="50">1</span>
                                    <span class="counter-label">Totoal calorie you need to consume</span>
                                </div>
                            </div>
                        }
                        else
                        {
                            <div class="col-md-3 col-sm-6 animate-box" data-animate-effect="fadeInUp">
                                <div class="feature-center">
                                    <span class="counter js-counter" data-from="0" data-to="@a" data-speed="3000" data-refresh-interval="50">1</span>
                                    <span class="counter-label">You still need @a KCal</span>
                                </div>
                            </div>
                        }
                    }
                    

                </div>
            </div>
        </div>
        <footer id="gtco-footer" role="contentinfo" style="background-image: url(images/img_bg_1.jpg)" data-stellar-background-ratio="0.5">
